<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MyActivity$PlaceholderFragment">

    <TextView
        android:text="@string/free_champ_rotation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp1"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/ic_launcher" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp2"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_alignTop="@+id/imgVChamp1"
        android:layout_toRightOf="@+id/imgVChamp1"
        android:layout_toEndOf="@+id/imgVChamp1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp3"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_toEndOf="@+id/imgVChamp2"
        android:layout_alignTop="@+id/imgVChamp2"
        android:layout_toRightOf="@+id/imgVChamp2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp4"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_alignTop="@+id/imgVChamp3"
        android:layout_toRightOf="@+id/imgVChamp3"
        android:layout_toEndOf="@+id/imgVChamp3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp5"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_toEndOf="@+id/imgVChamp4"
        android:layout_alignTop="@+id/imgVChamp4"
        android:layout_toRightOf="@+id/imgVChamp4" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp6"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_below="@+id/imgVChamp1"
        android:layout_alignLeft="@+id/imgVChamp1"
        android:layout_alignStart="@+id/imgVChamp1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp7"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_alignTop="@+id/imgVChamp6"
        android:layout_alignLeft="@+id/imgVChamp2"
        android:layout_alignStart="@+id/imgVChamp2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp8"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_toEndOf="@+id/imgVChamp7"
        android:layout_alignTop="@+id/imgVChamp7"
        android:layout_toRightOf="@+id/imgVChamp7" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp9"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_alignTop="@+id/imgVChamp8"
        android:layout_toRightOf="@+id/imgVChamp8"
        android:layout_toEndOf="@+id/imgVChamp8" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imgVChamp10"
        android:minHeight="@dimen/icons"
        android:minWidth="@dimen/icons"
        android:src="@drawable/ic_launcher"
        android:layout_toEndOf="@+id/imgVChamp9"
        android:layout_alignTop="@+id/imgVChamp9"
        android:layout_toRightOf="@+id/imgVChamp9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/game_patch_notes"
        android:id="@+id/txtVGamePatchNotes"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Patch note CONTAINER"
        android:id="@+id/txtVPatchNote"
        android:layout_below="@+id/txtVGamePatchNotes"
        android:layout_alignLeft="@+id/imgVChamp6"
        android:layout_alignStart="@+id/imgVChamp6" />


</RelativeLayout>
