        </activity>
        <activity
            android:name=".MyActivityLore"
            android:label="@string/title_activity_my_activity_lore" >
        </activity>
        <activity
            android:name=".MyActivityGenerateBuild"
            android:label="@string/title_activity_my_activity_generate_build" >
        </activity>
        <activity
            android:name=".MyActivityStatPriority"
            android:label="@string/title_activity_my_activity_stat_priority" >
